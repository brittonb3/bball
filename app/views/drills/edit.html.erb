<h1>Drills#edit</h1>
<p>Find me in app/views/drills/edit.html.erb</p>
<h3>Create new Drill</h3>
<%= form_for @drill, html: { multipart: true } do |f| %>
   <%= f.label :file %>
   <%= f.file_field :file %>

   <%= f.label :title %>
   <%= f.text_field :title %>

   <%= f.label :description %>
   <%= f.text_area :description %>

   <%= f.submit "Save" %>
 <% end %>
